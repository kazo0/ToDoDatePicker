<Application
    x:Class="ToDoDatePicker.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ToDoDatePicker">
	
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />

				<!-- Load Material Color Palette -->
				<MaterialColors xmlns="using:Uno.Material" />
				<MaterialFonts xmlns="using:Uno.Material" />

				<!-- Load the Material control resources -->
				<MaterialResources xmlns="using:Uno.Material" />

				<ResourceDictionary Source="ms-appx:///DatePicker.xaml" />
			</ResourceDictionary.MergedDictionaries>
            <!-- Add resources here -->
        </ResourceDictionary>
    </Application.Resources>

</Application>
